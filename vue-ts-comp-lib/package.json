{
  "name": "vue-ts-comp-lib",
  "version": "1.0.0",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-ts-comp-lib.umd.js",
  "module": "./dist/vue-ts-comp-lib.es.js",
  "types": "dist/@types/src/components/index.d.ts",
  "description": "Our first vue-ts library",
  "exports": {
    ".": {
      "import": "./dist/vue-ts-comp-lib.es.js",
      "require": "./dist/vue-ts-comp-lib.umd.js"
